select * from employees


SELECT FIRST_NAME,HIRE_DATE,JOB_ID
FROM EMPLOYEES
WHERE JOB_ID IN ('IT_PROG','SA_MAN')



SELECT EMPLOYEE_ID,FIRST_NAME
FROM EMPLOYEES
WHERE HIRE_DATE > '01-JAN-2008'


SELECT * FROM EMPLOYEES
WHERE EMPLOYEE_ID  IN (150,160)

SELECT FIRST_NAME,SALARY,COMMISSION_PCT,HIRE_DATE
FROM EMPLOYEES
WHERE SALARY < 10000


SELECT JOB_ID
MIN_SALARY , MAX_SALARY DIFFERENCE
FROM JOBS
WHERE MAX_SALARY BETWEEN 10000 AND 20000

SELECT FIRST_NAME,SALARY,ROUND(SALARY,-3)
FROM EMPLOYEES

SELECT * FROM JOBS ORDER BY JOB_TITLE

SELECT * FROM EMPLOYEES ORDER BY JOB_ID

 SELECT FIRST_NAME,LAST_NAME
 FROM EMPLOYEES
 WHERE FIRST_NAME LIKE'S%' OR LAST_NAME LIKE'%S'
 
 
 SELECT EMPLOYEE_ID,FIRST_NAME
 FROM EMPLOYEES
 WHERE TO_CHAR(HIRE_DATE,'MON')='MAY'
 
 
 SELECT * FROM EMPLOYEES
 
SELECT * FROM EMPLOYEES
 WHERE COMMISSION_PCT IS NULL
 AND SALARY BETWEEN 5000 AND 10000
 AND DEPARTMENT_ID=30
 
 
 SELECT FIRST_NAME , HIRE_DATE,LAST_DAY(HIRE_DATE+1) 
 FROM EMPLOYEES
 
 
 SELECT FIRST_NAME,HIRE_DATE,FLOOR((SYSDATE-HIRE_DATE)/365)
 FROM EMPLOYEES
 
 
 SELECT FIRST_NAME 
 FROM EMPLOYEES
 WHERE TO_CHAR(HIRE_DATE,'YYYY')=2001
 
 
 SELECT INITCAP(FIRST_NAME) , INITCAP(LAST_NAME)
 FROM EMPLOYEES

 
 SELECT JOB_ID,
 SUBSTR(JOB_ID,1,INSTR(JOB_ID,'')-1)
 FROM EMPLOYEES
 
 
  SELECT FIRST_NAME,LAST_NAME
 FROM EMPLOYEES
 WHERE INSTR(LAST_NAME,'B') > 3
 
 SELECT * FROM EMPLOYEES
 
 
 SELECT UPPER(FIRST_NAME),LOWER(EMAIL)
 FROM EMPLOYEES
 WHERE UPPER(FIRST_NAME)=UPPER(EMAIL)
 
 
 SELECT * FROM EMPLOYEES
 WHERE TO_CHAR(HIRE_DATE,'YYYY')=TO_CHAR(SYSDATE,'YYYY')
 
 
 SELECT * FROM EMPLOYEES
 WHERE TO_CHAR(HIRE_DATE,'YYYY')=2001
 
 
 SELECT SYSDATE-TO_DATE('01-JAN-2011')
 FROM DUAL
 
SELECT TO_CHAR(HIRE_DATE,'MM')
FROM EMPLOYEES
WHERE TO_CHAR(HIRE_DATE,'YYYY')=TO_CHAR(SYSDATE,'YYYY')
GROUP BY(HIRE_DATE,'MM')


SELECT MANAGER_ID,COUNT(*)
FROM EMPLOYEES
GROUP BY MANAGER_ID


SELECT EMPLOYEE_ID,MAX(END_DATE)
FROM JOB_HISTORY 
GROUP BY EMPLOYEE_ID

SELECT EMPLOYEE_ID,HIRE_DATE,FIRST_DATE(HIRE_DATE)-1 FROM EMPLOYEES


SELECT COUNT(*) FROM EMPLOYEES
WHERE TO_CHAR(HIRE_DATE,'DD')>15

SELECT COUNTRY_ID,COUNT(*)
FROM LOCATIONS
GROUP BY COUNTRY_ID

SELECT * FROM LOCATIONS

SELEC * CAT
SELECT * FROM CAT

SELECT DEPARTMENT_ID ,AVG(SALARY)
FROM EMPLOYEES
WHERE COMMISSION_PCT IS NOT NULL 
GROUP BY DEPARTMENT_ID


SELECT JOB_ID,COUNT(*) EMPLOYEE_ID,SUM(SALARY),
MAX(SALARY)-MIN(SALARY)
FROM EMPLOYEES
GROUP BY JOB_ID



SELECT JOB_ID,AVG(SALARY)
FROM EMPLOYEES
GROUP BY JOB_ID
HAVING AVG(SALARY)>10000


SELECT TO_CHAR(HIRE_DATE,'YYYY')
FROM EMPLOYEES
GROUP BY TO_CHAR(HIRE_DATE,'YYYY')
HAVING COUNT(EMPLOYEE_ID)>10

SELECT DEPARTMENT_ID 
FROM EMPLOYEES
WHERE COMMISSION_PCT IS NOT NULL
GROUP BY DEPARTMENT_ID
HAVING COUNT(COMMISSION_PCT)>5


SELECT * FROM JOB_HISTORY


SELECT * FROM JOBS


SELECT JOB_ID 
FROM JOB_HISTORY
WHERE END_DATE-START_DATE>100
GROUP BY JOB_ID
HAVING COUNT(*)>3


SELECT DEPARTMENT_ID,TO_CHAR(HIRE_DATE,'YYYY'),COUNT(EMPLOYEE_ID)
FROM EMPLOYEES
GROUP BY DEPARTMENT_ID,
TO_CHAR(HIRE_DATE,'YYYY')
ORDER BY DEPARTMENT_ID



SELECT DEPARTMENT_ID
FROM EMPLOYEES
GROUP BY DEPARTMENT_ID,MANAGER_ID 
HAVING COUNT(EMPLOYEE_ID)>5

UPDATE EMPLOYEES SET SALARY=8000
WHERE EMPLOYEE_ID=115 AND SALARY<6000


